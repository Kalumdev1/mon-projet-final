{
  "extends": [
    "development"
  ],
  "hints": {
    "no-inline-styles": "off",
    "axe/name-role-value": [
      "default",
      {
        "button-name": "off",
        "link-name": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "backdrop-filter",
          "min-height: auto",
          "-webkit-overflow-scrolling"
        ]
      }
    ],
    "disown-opener": "off",
    "meta-viewport": "off",
    "axe/forms": [
      "default",
      {
        "select-name": "off"
      }
    ],
    "axe/text-alternatives": [
      "default",
      {
        "frame-title": "off",
        "image-alt": "off"
      }
    ],
    "axe/structure": [
      "default",
      {
        "list": "off"
      }
    ],
    "axe/aria": [
      "default",
      {
        "aria-required-children": "off",
        "aria-allowed-attr": "off"
      }
    ]
  }
}